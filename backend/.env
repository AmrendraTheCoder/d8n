# D8N Backend Environment Variables

# MongoDB
MONGO_URI=mongodb+srv://your_user:your_password@cluster.mongodb.net/?retryWrites=true&w=majority

# Master Wallet (for executing payments)
# WARNING: Never commit this file with real private key!
MASTER_WALLET_PRIVATE_KEY=0x_your_private_key_here

# RPC URLs
CRONOS_RPC_URL=https://testnet.zkevm.cronos.org
BASE_RPC_URL=https://sepolia.base.org
POLYGON_RPC_URL=https://rpc-amoy.polygon.technology
SEPOLIA_RPC_URL=https://rpc.sepolia.org

# Server
PORT=3001
